<h1>Welcome to Makers IDE</h1>

<form class="new-file">
<input type="text" name="file-name">
<button class="add-file">+ New File</button>
<style>

</style>
</form>

<ul class="files">
	<% files.forEach(function(file){ %>
		<li>
			<span><%- file %></span>
			<button class="edit">Edit</button>
			<button class="delete">Delete</button>
		</li>
	<% }) %>
</ul>

